{
  "kind": "collectionType",
  "collectionName": "jokes",
  "info": {
    "name": "Joke",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "content": {
      "type": "text",
      "required": true
    },
    "ip_address": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "community_approved",
        "community_rejected",
        "admin_approved",
        "admin_rejected"
      ],
      "default": "pending",
      "required": true
    },
    "moderated_up": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "moderated_down": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "jokes"
    },
    "joke_moderations": {
      "collection": "joke-moderations"
    },
    "comments": {
      "private": true,
      "collection": "comment"
    },
    "tags": {
      "collection": "tag"
    }
  }
}
